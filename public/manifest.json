{
    "name": "Change Theme Extension",
    "description": "This extension is made for demonstration purposes",
    "version": "1.0",
    "manifest_version": 3,
    "minimum_chrome_version": "92",
    "permissions": [
       "activeTab",
       "scripting"
    ],
    "action":{
       "default_popup": "popup.html"
    },
    "background": {
       "service_worker": "./js/background.js",
       "type": "module"
    }
 }

